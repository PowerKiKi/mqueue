{
    "name": "powerkiki/mqueue",
    "description": "Web application to queue movies to watch",
    "license": "MIT",
    "authors": [
        {
            "name": "PowerKiKi",
            "email": "adrien.crivelli@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "mQueue\\": "application/mQueue/"
        },
        "classmap": [
            "application/controllers/",
            "application/Bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "mQueueTest\\": "tests/mQueueTest"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "~7.2.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-posix": "*",
        "zendframework/zendframework1": "^1.12"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "@stable",
        "pcov/clobber": "@stable",
        "phpstan/phpstan": "@stable",
        "phpunit/phpunit": "^8.5"
    }
}
